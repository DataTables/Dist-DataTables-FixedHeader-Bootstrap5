<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>3.2.2</version>
        <id>datatables.net-fixedheader-bs5</id>
        <description>This is FixedHeader for DataTables with styling for [Bootstrap5](https://getbootstrap.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-FixedHeader-Bootstrap5.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>FixedHeader  Fixed Headers  Sticky  Bootstrap  Bootstrap5  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-fixedheader" version="3.2.2"/>
            <dependency id="datatables.net-bs5" version="1.11.5"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/fixedHeader.bootstrap5.js" target="content\Scripts"/>
        <file src="js/fixedHeader.bootstrap5.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/fixedHeader.bootstrap5.min.js" target="content\Scripts"/>
        <file src="js/fixedHeader.bootstrap5.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/fixedHeader.bootstrap5.css" target="content\Content"/>
        <file src="css/fixedHeader.bootstrap5.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/fixedHeader.bootstrap5.min.css" target="content\Content"/>
        <file src="css/fixedHeader.bootstrap5.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

